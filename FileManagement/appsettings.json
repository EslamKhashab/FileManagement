{
  "ImageSettings": {
    "MaxBytes": 2097152,
    "AcceptedFileTypes": [ "image/jpeg", "image/png", "image/webp" ]
  },
  "AttachmentSettings": {
    "StorageFolder": "storage",
    "Sizes": {
      "phone": 480,
      "tablet": 768,
      "desktop": 1920
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}